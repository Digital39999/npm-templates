{
	"version": "1.0.0",
	"name": "digital-template",
	"author": "Digital39999",
	"scripts": {
		"ts": "pnpm install typescript --save-dev -g",
		"init": "pnpm install",
		"lint": "eslint src --fix --ext .ts",
		"start": "node dist/index.js",
		"build": "tsc --build",
		"watch": "tsc --watch",
		"rebuild": "pnpm run build && pnpm run start",
		"updates": "pnpm run npm-check-updates -i --format group"
	},
	"repository": "https://github.com/Digital39999/whatever",
	"main": "dist/index.js",
	"type": "commonjs",
	"license": "MIT",
	"dependencies": {
		"discord.js": "^14.14.1",
		"dotenv": "^16.3.1",
		"status-sharding": "^1.5.4",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/node": "^20.14.13",
		"@typescript-eslint/eslint-plugin": "^6.18.0",
		"@typescript-eslint/parser": "^6.18.0",
		"eslint": "^8.56.0",
		"npm-check-updates": "^16.14.12",
		"typescript": "^5.3.3"
	},
	"engines": {
		"node": ">=20.2.0"
	}
}